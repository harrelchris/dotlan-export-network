{
  "manifest_version": 3,
  "name": "Dotlan Export Network",
  "version": "0.1.0",
  "description": "Add a button to Dotlan to export Jump Bridge networks",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "content_scripts": [{
    "js": [
      "content-script.js"
    ],
    "matches": [
      "https://evemaps.dotlan.net/bridges/*"
    ]
  }]
}